pipeline {
  agent any
  stages {
        stage('Repository Checkout') {
          steps {
            echo 'Repository Checkout...'
          }
        }

        stage('War file') {
          steps {
            echo 'War file...'
          }
        }

        stage('publish') {
          steps {
            echo 'publish'
          }
        }

    }

    stage('build') {
      steps {
        echo 'build'
      }
    }

    stage('run') {
      steps {
        echo 'run'
      }
    }

    stage('done') {
      steps {
        echo 'done'
      }
    }
}
